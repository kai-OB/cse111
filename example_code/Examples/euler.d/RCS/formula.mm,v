1.10
log
@-
@
text
@.fam N
.ds pointsize 17.28
.ds vspacing 20.736
.ds eqnsize 20.736
.S \*[pointsize] \*[vspacing]
.de TP
.   sp 5v
..
.B "Euler's Formula & Identity"
.sp
.EQ
size \*[eqnsize] {
   pi ~ ~ = ~ ~ roman { acos } ~ ( -1 )
      ~ ~ = ~ ~ 4 ~ roman { atan } ~ ( 1 )
      ~ ~ = ~ ~ roman { atan2 } ~ ( 0 , -1 )
}
.EN
.sp
.EQ
size \*[eqnsize] { i ~ ~ = ~ ~ sqrt { -1 } }
.EN
.sp 2v
.B "Euler's Formula\|:"
.sp
.EQ
size \*[eqnsize] { e sup { i x } ~ ~ = ~ ~ cos x + i sin x }
.EN
.sp 2v
.B "Euler's Identity\|:"
.sp
.EQ
size \*[eqnsize] { e sup { i pi } + 1 ~ ~ = ~ ~ 0 }
.EN
.sp 2v
.B "Golden Ratio\|:"
.sp
.EQ
size \*[eqnsize] { Phi ~ ~ = ~ ~ { 1 + sqrt 5 } over 2 }
.EN
@


