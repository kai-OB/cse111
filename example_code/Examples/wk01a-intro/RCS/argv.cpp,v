1.9
log
@-
@
text
@// $Id: argv.cpp,v 1.8 2018-06-20 14:17:52-07 - - $

//
// NAME
//    argv - print out the argument vector
//
// SYNOPSIS
//    argv [string...]
//
// DESCRIPTION
//    Prints out its arguments to stdout, one per line.
//

#include <cstdlib>
#include <iostream>

using namespace std;

int main (int argc, char** argv) {
   cout << "argc = " << argc << endl;
   cout << "argv = " << argv << endl;
   for (int argi = 0; argi != argc; ++argi) {
      cout << "argv[" << argi << "] = "
           << static_cast<const void*> (argv[argi]) << "->\""
           << argv[argi] << "\"" << endl;
   }
   return EXIT_SUCCESS;
}

//TEST// ./argv foo bar baz qux quux >argv.lis 2>&1
//TEST// mkpspdf argv.ps argv.cpp* argv.lis

@


